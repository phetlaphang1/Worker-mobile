# Server Configuration
PORT=5051
NODE_ENV=development

# LDPlayer Paths
LDCONSOLE_PATH=D:\LDPlayer\LDPlayer9\ldconsole.exe
ADB_PATH=D:\LDPlayer\LDPlayer9\adb.exe

# Task Center Configuration (Required for script automation)
TASK_CENTER_URL=http://localhost:3000/api/tasks
TASK_CENTER_API_KEY=your_api_key_here
TASK_CENTER_USER_ID=your_user_id

# Appium Configuration (Required for script automation)
APPIUM_SERVER_URL=http://127.0.0.1:4723

# Auto-Install Apps (true/false)
AUTO_INSTALL_TWITTER=true
AUTO_INSTALL_FACEBOOK=true
AUTO_INSTALL_INSTAGRAM=true
AUTO_INSTALL_TELEGRAM=true
AUTO_INSTALL_TIKTOK=true

# APK Paths (relative to Worker-mobile folder)
TWITTER_APK_PATH=./apks/twitter.apk
FACEBOOK_APK_PATH=./apks/facebook.apk
INSTAGRAM_APK_PATH=./apks/instagram.apk
TELEGRAM_APK_PATH=./apks/telegram.apk
TIKTOK_APK_PATH=./apks/tiktok.apk

# System Configuration
MAX_CONCURRENT_TASKS=5
TASK_CHECK_INTERVAL=30000
AUTO_REFRESH_INTERVAL=5000
MAX_CONCURRENT_INSTANCES=10

# Script Execution Configuration
SCRIPT_TIMEOUT=600000
MAX_RETRY_ATTEMPTS=3

# Logging
LOG_LEVEL=info
LOG_FILE=logs/worker-mobile.log

# Instance Pool Manager Configuration
POOL_MIN_INSTANCES=1
POOL_MAX_INSTANCES=20
POOL_TARGET_INSTANCES=5
POOL_AUTO_SCALE=true
POOL_HEALTH_CHECK_INTERVAL=60000
POOL_INSTANCE_TEMPLATE=Master

# Instance Default Configuration
INSTANCE_RESOLUTION=360x640
INSTANCE_DPI=240
INSTANCE_CPU=2
INSTANCE_MEMORY=2048

# Proxy Configuration
PROXY_ENABLED=false
PROXY_CONFIG_FILE=proxies.json
PROXY_AUTO_ASSIGN=true
PROXY_ROTATION_STRATEGY=round-robin
PROXY_MAX_USAGE_PER_PROXY=100
