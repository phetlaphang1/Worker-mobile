{
  "name": "worker-mobile",
  "version": "1.0.0",
  "description": "Mobile automation worker for LDPlayer Android emulator",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\" \"npm run dev:watch\"",
    "dev:server": "nodemon --watch server --ext ts,js --exec tsx server/index.ts",
    "dev:client": "cd client && vite",
    "dev:watch": "echo 'Hot reload enabled! Server: http://localhost:5051 | Client dev: http://localhost:5173'",
    "dev:server-only": "tsx server/index.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd client && vite build",
    "build:server": "tsc --project server/tsconfig.json",
    "start": "node dist/index.js",
    "test": "node test-boxphone.js",
    "test:ldplayer": "node test-ldplayer.js",
    "check": "node check-system.js",
    "detect-ldplayer": "tsx server/utils/detectLDPlayer.ts"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@uiw/react-textarea-code-editor": "^3.1.1",
    "@wdio/appium-service": "^8.27.0",
    "@wdio/local-runner": "^8.27.0",
    "adb-ts": "^2.0.0",
    "appium": "^2.4.1",
    "axios": "^1.6.5",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsoneditor": "^10.4.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.453.0",
    "node-schedule": "^2.1.1",
    "proxy-chain": "^2.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reactflow": "^11.11.4",
    "sharp": "^0.33.1",
    "tailwind-merge": "^2.6.0",
    "tesseract.js": "^5.0.4",
    "uuid": "^9.0.1",
    "webdriverio": "^8.27.0",
    "winston": "^3.11.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.3.1",
    "@types/ws": "^8.5.10",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^7.1.3"
  }
}
